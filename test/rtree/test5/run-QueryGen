#! /bin/bash

# NOTE(utku): You can not run this file standalone. This file is called from "run-GenerateQueriesAndExecOnDyn-STR" in which "ds data_loc_dist qs ..." are set (i.e. exported). 

echo Generating queryset  #"Usage: ds data_loc_dist qs query_loc_dist d_dx d_dy d_dist q_dx q_dy q_dist" 

mkdir -p  ../../debug/query  # -p flag: mk dir only if dir does not exist.
export querydir=../../debug/query

export qpostfix=${qs}_${query_loc_dist}_${qx}_${qy}
export queryfile=${querydir}/query${qpostfix}

export bindir=../../debug
export dbdir=../../debug/database/${ds}

