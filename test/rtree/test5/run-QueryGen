export pltdir=../../debug/plt

mkdir -p ../../debug/results
export resultsdir=../../debug/results


#time ../../test-rtree-Generator $ds $data_loc_dist $qs $query_loc_dist $dx $dy $d_dist $qx $qy $q_dist > d
# time ../../test-rtree-Generator 1 u $qs $query_loc_dist 0.1 0.1 f $qx $qy $q_dist > d   # 1 u .. 0.1 0.1 f is dummy

export aqar=0.1
./GenQry

export aqar=0.3
./GenQry

export aqar=0.7
./GenQry

export aqar=1
./GenQry

export aqar=1.4
./GenQry

export aqar=3.3
./GenQry

export aqar=10
